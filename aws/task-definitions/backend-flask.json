{
    "family": "backend-flask",
    "executionRoleArn": "arn:aws:iam::074066800719:role/CruddurServiceExecutionRole",
    "taskRoleArn": "arn:aws:iam::074066800719:role/CruddurTaskRole",
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
        {
            "name": "backend-flask",
            "image": "074066800719.dkr.ecr.us-east-1.amazonaws.com/backend-flask",
            "essential": true,
            "portMappings": [
                {
                    "name": "backend-flask",
                    "containerPort": 4567,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "cruddur",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "backend-flask"
                }
            },
            "environment": [
                {
                    "name": "OTEL_SERVICE_NAME",
                    "value": "backend-flask"
                },
                {
                    "name": "OTEL_EXPORTER_OTLP_ENDPOINT",
                    "value": "https://api.honeycomb.io"
                },
                {
                    "name": "AWS_COGNITO_USER_POOL_ID",
                    "value": "us-east-1_tnTyU38uf"
                },
                {
                    "name": "AWS_COGNITO_USER_POOL_CLIENT_ID",
                    "value": "27gkk1cq0g57t0jo37rc11c47h"
                },
                {
                    "name": "FRONTEND_URL",
                    "value": ""
                },
                {
                    "name": "BACKEND_URL",
                    "value": ""
                },
                {
                    "name": "AWS_DEFAULT_REGION",
                    "value": "us-east-1"
                }
            ],
            "secrets": [
                {
                    "name": "AWS_ACCESS_KEY_ID",
                    "valueFrom": "arn:aws:ssm:us-east-1:074066800719:parameter/cruddur/backend-flask/AWS_ACCESS_KEY_ID"
                },
                {
                    "name": "AWS_SECRET_ACCESS_KEY",
                    "valueFrom": "arn:aws:ssm:us-east-1:074066800719:parameter/cruddur/backend-flask/AWS_SECRET_ACCESS_KEY"
                },
                {
                    "name": "CONNECTION_URL",
                    "valueFrom": "arn:aws:ssm:us-east-1:074066800719:parameter/cruddur/backend-flask/CONNECTION_URL"
                },
                {
                    "name": "ROLLBAR_ACCESS_TOKEN",
                    "valueFrom": "arn:aws:ssm:us-east-1:074066800719:parameter/cruddur/backend-flask/ROLLBAR_ACCESS_TOKEN"
                },
                {
                    "name": "OTEL_EXPORTER_OTLP_HEADERS",
                    "valueFrom": "arn:aws:ssm:us-east-1:074066800719:parameter/cruddur/backend-flask/OTEL_EXPORTER_OTLP_HEADERS"
                }
            ]
        }
    ]
}